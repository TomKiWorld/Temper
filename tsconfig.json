{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "specs/*.ts",],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["es6", "dom", "es2022"],
    "target": "es5",
    "ignoreDeprecations": "5.0",
    "types": ["vitest/importMeta"]
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
